{
    "name": "r94ever/laravel-model-setting",
    "description": "Add setting function to Laravel models.",
    "keywords": [
        "r94ever",
        "laravel-model-setting",
        "laravel-model"
    ],
    "homepage": "https://github.com/r94ever/laravel-model-setting",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Duong Thanh Van",
            "email": "vandt147@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "r94ever\\LaravelModelSetting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "r94ever\\LaravelModelSetting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "r94ever\\LaravelModelSetting\\ModelSettingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
